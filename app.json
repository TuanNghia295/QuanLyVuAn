{
  "expo": {
    "name": "QuanLyVuAn",
    "slug": "QuanLyVuAn",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/quanLyVuAn.png",
    "scheme": "quanlyvuan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.thome.QuanLyVuAn",
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Ứng dụng cần quyền thông báo để gửi các cập nhật quan trọng.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tuannghia2905.QuanLyVuAn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "mode": "production",
          "icon": "./assets/images/quanLyVuAn.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2980fa67-9e1f-44d1-9437-d971e9277bba"
      }
    },
    "owner": "tuannghia2905"
  }
}
